{
  "name": "i18n-for-jt",
  "displayName": "i18n for JT",
  "description": "国际化插件",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.66.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:i18n-for-jt.helloWorld"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "keybindings": [
      {
        "command": "i18n-for-jt.helloWorld",
        "key": "ctrl+shift+j",
        "mac": "cmd+shift+t",
        "when": "editorHasSelection"
      }
    ],
    "configuration": {
      "title": "i18n-for-jt",
      "properties": {
        "i18n-for-jt.config": {
          "type": "object",
          "default": {
            "zhCNPath": "\\src\\locales\\zh_CN.json",
            "enUSPath": "\\src\\locales\\en_US.json",
            "replaceContent": "{intl.get('$1').d('$2')}"
          },
          "description": "传入一个对象，zhCNPath:中文语言包路径;enUSPath:英文语言包路径(windos系统路径分隔使用'\\');replaceContent:国际化插件需要的标识($1:语言包中的key,$2:可选，中文含义)。详细介绍可以看readme"
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.2.0",
    "@types/mocha": "^9.1.0",
    "@types/node": "14.x",
    "@types/vscode": "^1.66.0",
    "@typescript-eslint/eslint-plugin": "^5.16.0",
    "@typescript-eslint/parser": "^5.16.0",
    "@vscode/test-electron": "^2.1.3",
    "eslint": "^8.11.0",
    "glob": "^7.2.0",
    "md5": "^2.3.0",
    "mocha": "^9.2.2",
    "typescript": "^4.5.5"
  },
  "dependencies": {},
  "repository": "https://github.com/Mr-Nobody-li/i18n-for-jt",
  "publisher": "MrNobody"
}
